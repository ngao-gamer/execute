    // --- Lệnh Lua phức tạp để tạo thông báo tùy chỉnh (2 thông báo chồng) ---
    string customExecutedNotificationScript = @"
local TweenService = game:GetService('TweenService') 
local Players = game:GetService('Players')

local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild('PlayerGui')
local NotificationPrefix = 'ExecutedNotification_'
local MaxNotifications = 4 -- Tăng giới hạn để chứa 2 thông báo mới
local Padding = 10 -- Khoảng cách giữa các thông báo

-- Tham số cho thông báo CHÍNH (Purium Injected)
local FRAME_WIDTH_MAIN = 230  -- Nhỏ hơn 250px cũ
local FRAME_HEIGHT_MAIN = 40  -- Nhỏ hơn 45px cũ
local BORDER_THICKNESS_MAIN = 5
local DURATION_MAIN = 2.5 -- Thời gian hiển thị 2.5 giây

-- Tham số cho thông báo PHỤ (Version)
local FRAME_WIDTH_SUB = 115   -- Bằng 1/2 thông báo chính (115px)
local FRAME_HEIGHT_SUB = 25
local BORDER_THICKNESS_SUB = 2
local DURATION_SUB = 2.0 -- Thời gian hiển thị 2.0 giây

local BAR_HEIGHT = 2 -- Chiều cao thanh tiến trình (2px)

-- Hàm tính toán vị trí X (góc phải) và Y (từ trên xuống)
local function getTargetPosition(index, width)
    -- Vị trí dịch chuyển xuống: 20px (margin trên) + (index - 1) * (FRAME_HEIGHT_MAIN + Padding)
    local yOffset = 20 + (index - 1) * (FRAME_HEIGHT_MAIN + Padding)
    -- Vị trí X: 1 - (chiều rộng + 20px margin phải)
    local xOffset = -(width + 20) 
    return UDim2.new(1, xOffset, 0, yOffset) 
end

-- Lấy tất cả thông báo đang hoạt động
local function getActiveNotifications()
    local notifications = {}
    for _, child in ipairs(PlayerGui:GetChildren()) do
        if child.Name:sub(1, #NotificationPrefix) == NotificationPrefix and child:IsA('ScreenGui') then
            table.insert(notifications, child)
        end
    end
    -- Sắp xếp theo thứ tự tạo
    table.sort(notifications, function(a, b)
        local timeA = tonumber(a.Name:match(NotificationPrefix .. '(%d+)')) or 0
        local timeB = tonumber(b.Name:match(NotificationPrefix .. '(%d+)')) or 0
        return timeA < timeB
    end)
    return notifications
end

-- Hàm quan trọng: Dịch chuyển tất cả thông báo lên/xuống
local function recalculateAndShiftAll()
    local currentNotifications = getActiveNotifications()
    local yOffsetCounter = 20
    
    for index, notification in ipairs(currentNotifications) do
        local frame = notification:FindFirstChild('Frame')
        if frame then
            local isMain = frame:GetAttribute('IsMain') == 'true'
            local frameHeight = isMain and FRAME_HEIGHT_MAIN or FRAME_HEIGHT_SUB
            local frameWidth = isMain and FRAME_WIDTH_MAIN or FRAME_WIDTH_SUB
            
            -- Vị trí Y: Dựa trên yOffsetCounter
            local targetPos = UDim2.new(1, -(frameWidth + 20), 0, yOffsetCounter)
            
            local ShiftInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out) 
            TweenService:Create(frame, ShiftInfo, { Position = targetPos }):Play()
            
            -- Tăng bộ đếm cho thông báo tiếp theo
            yOffsetCounter = yOffsetCounter + frameHeight + Padding
        end
    end
end

-- Hàm xử lý hiệu ứng đóng và dọn dẹp
local function dismissNotification(notification, shouldShiftUp)
    local Frame = notification:FindFirstChild('Frame')
    if not Frame or not Frame.Parent then return end 

    -- Prep-Exit (Nhún nhẹ ra ngoài)
    local PrepExitInfo = TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    local PrepExitGoal = {
        Position = UDim2.new(1.05, -(Frame.Size.X.Offset + 20), Frame.Position.Y.Scale, Frame.Position.Y.Offset) 
    }
    TweenService:Create(Frame, PrepExitInfo, PrepExitGoal):Play().Completed:Wait() 
    
    -- Retreat (Trượt hẳn ra ngoài)
    local RetreatInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
    local RetreatGoal = { Position = UDim2.new(1.1, 0, Frame.Position.Y.Scale, Frame.Position.Y.Offset) }
    TweenService:Create(Frame, RetreatInfo, RetreatGoal):Play().Completed:Wait() 
    
    notification:Destroy()
    
    -- QUAN TRỌNG: Dịch chuyển các thông báo còn lại lên
    if shouldShiftUp then
        recalculateAndShiftAll()
    end
end

-- Logic tạo và chạy thông báo đơn
local function createSingleNotification(text1, text2, width, height, border, duration, isMain)
    local currentNotifications = getActiveNotifications()

    -- 1. XỬ LÝ GIỚI HẠN TỐI ĐA
    if #currentNotifications >= MaxNotifications then
        local oldest = currentNotifications[1]
        if oldest then oldest:Destroy() end
    end
    
    -- 2. TẠO GUI VÀ FRAME MỚI
    local Notification = Instance.new('ScreenGui')
    Notification.Name = NotificationPrefix .. tostring(math.floor(tick() * 1000)) 
    Notification.DisplayOrder = 999 

    local Frame = Instance.new('Frame')
    Frame.Size = UDim2.new(0, width, 0, height) 
    Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    Frame.BorderSizePixel = border 
    Frame.BorderColor3 = Color3.fromRGB(30, 144, 255)
    Frame.Draggable = false 
    Frame:SetAttribute('IsMain', isMain and 'true' or 'false') -- Đánh dấu loại Frame
    Frame.Parent = Notification
    
    local Corner = Instance.new('UICorner')
    Corner.CornerRadius = UDim.new(0, 8) 
    Corner.Parent = Frame

    -- Label Nội dung
    local Header = Instance.new('TextLabel')
    Header.Text = text1
    Header.TextColor3 = Color3.fromRGB(30, 144, 255) 
    Header.TextSize = isMain and 14 or 12
    Header.Font = Enum.Font.SourceSansBold
    Header.BackgroundTransparency = 1
    Header.Size = UDim2.new(1, 0, 0.5, 0)
    Header.TextXAlignment = Enum.TextXAlignment.Left
    Header.TextYAlignment = Enum.TextYAlignment.Top
    Header.Position = UDim2.new(0.05, 0, isMain and 0.1 or 0, 0)
    Header.Parent = Frame
    
    local Content = Instance.new('TextLabel')
    Content.Text = text2
    Content.TextColor3 = Color3.fromRGB(173, 255, 47) 
    Content.TextSize = isMain and 16 or 12
    Content.Font = Enum.Font.SourceSansBold
    Content.BackgroundTransparency = 1
    Content.Size = UDim2.new(1, 0, 0.5, 0)
    Content.TextXAlignment = Enum.TextXAlignment.Left
    Content.TextYAlignment = Enum.TextYAlignment.Bottom
    Content.Position = UDim2.new(0.05, 0, isMain and 0.5 or 0.5, 0)
    Content.Parent = Frame
    
    -- Thanh Tiến Trình (2px Lime)
    local ProgressBar = Instance.new('Frame')
    ProgressBar.Name = 'ProgressBar'
    ProgressBar.Size = UDim2.new(1, 0, 0, BAR_HEIGHT)
    ProgressBar.Position = UDim2.new(0, 0, 1, -BAR_HEIGHT)
    ProgressBar.BackgroundColor3 = Color3.fromRGB(173, 255, 47) 
    ProgressBar.BorderSizePixel = 0
    ProgressBar.Parent = Frame

    Notification.Parent = PlayerGui
    
    -- *** XUẤT HIỆN VÀ NHÚN NHẢY ***
    
    -- Đặt vị trí khởi điểm ngoài màn hình
    local currentMaxIndex = #getActiveNotifications() 
    local currentYOffset = getTargetPosition(currentMaxIndex, width).Y.Offset
    Frame.Position = UDim2.new(1.1, 0, 0, currentYOffset) 
    
    -- TÍNH TOÁN LẠI VỊ TRÍ ĐÍCH
    recalculateAndShiftAll()
    local finalTargetPos = Frame.Position

    -- Overshoot (Trượt quá)
    local OvershootInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quart, Enum.EasingDirection.Out)
    local OvershootGoal = {
        Position = UDim2.new(1, -(width + 30), finalTargetPos.Y.Scale, finalTargetPos.Y.Offset) 
    }
    TweenService:Create(Frame, OvershootInfo, OvershootGoal):Play().Completed:Wait()
    
    -- Retract (Rút về vị trí cuối)
    local RetractInfo = TweenInfo.new(0.15, Enum.EasingStyle.Quart, Enum.EasingDirection.In)
    local RetractGoal = {
        Position = UDim2.new(1, -(width + 20), finalTargetPos.Y.Scale, finalTargetPos.Y.Offset) 
    }
    TweenService:Create(Frame, RetractInfo, RetractGoal):Play()
    
    -- *** TIẾN TRÌNH VÀ TỰ ĐÓNG ***
    
    local ProgressInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
    local ProgressGoal = { Size = UDim2.new(0, 0, 0, BAR_HEIGHT) }
    
    local ProgressTween = TweenService:Create(ProgressBar, ProgressInfo, ProgressGoal)
    ProgressTween:Play()
    ProgressTween.Completed:Wait()
    
    dismissNotification(Notification, true)
end

-- Chạy logic tạo 2 thông báo
-- 1. Thông báo chính (Injected)
createSingleNotification(
    'Purium Executor', 
    'Injected ✅', 
    FRAME_WIDTH_MAIN, 
    FRAME_HEIGHT_MAIN, 
    BORDER_THICKNESS_MAIN, 
    DURATION_MAIN,
    true
)

-- 2. Thông báo phụ (Version)
createSingleNotification(
    'Version', 
    '1.0.5', 
    FRAME_WIDTH_SUB, 
    FRAME_HEIGHT_SUB, 
    BORDER_THICKNESS_SUB, 
    DURATION_SUB,
    false
)
";
